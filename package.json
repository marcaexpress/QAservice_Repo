{
  "name": "qa-services",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:vercel": "cd apps/web && npm run build",
    "prebuild:vercel": "tailwindcss -c apps/web/tailwind.config.js -i apps/web/app/globals.css -o apps/web/public/_tw.css --minify",
    "build:strict": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "db:seed:dev": "turbo run db:seed:dev",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.540.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "playwright": "^1.54.2",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "tsx": "^4.20.4",
    "turbo": "^1.12.4"
  },
  "packageManager": "npm@10.2.4"
}
